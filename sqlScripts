CREATE TABLE products(
  id INT AUTO_INCREMENT,
  title VARCHAR 255,
  price double NOT NULL,
  description TEXT NOT NULL,
  imageUrl VARCHAR 255 NOT NULL,
  CONSTRAINT id_pk PRIMARY KEY id
  );

